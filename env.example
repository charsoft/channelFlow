# External API keys
YOUTUBE_API_KEY=YOUR_YOUTUBE_API_KEY_HERE
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Project & Bucket Configuration
GOOGLE_CLOUD_PROJECT=your-gcp-project-id
GCS_BUCKET_NAME=your-gcs-bucket-name
GCP_REGION=your-gcp-region
LOCATION=your-gcp-location
ENABLE_AUTO_INGESTION=false
TARGET_CHANNEL_ID=your-target-channel-id

# AI Model Configurations
GEMINI_MODEL_NAME=your-gemini-model-name
IMAGEN_MODEL_NAME=your-imagen-model-name

# Google Client OAuth (Frontend Auth)
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Server-side Secrets
JWT_SECRET_KEY="your-jwt-secret-key"
SECRET_KEY="your-general-secret-key"

# Google Service Account
GOOGLE_APPLICATION_CREDENTIALS=/app/service-account-key.json
